  pod 'AppCenter'
  pod 'SwiftGen'
  pod 'SnapKit',      '~> 5.0.0'
  pod 'SVProgressHUD'
  pod 'XCGLogger'
  pod 'SwiftyJSON', '~> 4.0'
  pod 'IQKeyboardManagerSwift'
  pod 'Firebase/Core'
  pod 'Firebase/Auth'
  pod 'Firebase/Database'
  pod 'FacebookCore'
  pod 'FacebookLogin'
  pod 'FacebookShare'
  pod 'GoogleSignIn'
  pod 'AppCenter'
  pod 'OneSignal', '>= 2.6.2', '< 3.0'

end

target 'OneSignalNotificationServiceExtension' do
    use_frameworks!
    
    pod 'OneSignal', '>= 2.6.2', '< 3.0'
